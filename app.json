{
  "expo": {
    "name": "Efols",
    "slug": "FrontApi5",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Backlog.png",
    "splash": {
      "image": "./src/assets/Backlog.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F2F2"
    },
    "notification": {
      "icon": "./src/assets/Backlog.png",
      "color": "#deff00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.Fatec.Efols",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
